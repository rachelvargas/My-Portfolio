#index.html
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>WDD 330 portfolio</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1>Week 01 Notes</h1>
    <h2>Section Tag</h2>
    <h3><section></h3>
      <p>Section tag fill the screen all the way across.</p>
      <p>Since the section tag requieres the full width of the page, the buttons can't be on the same line as the two inputs</p><br>
      <h2>When inputs</h2>
      <p>When we want to get the user's input from any 'input' or 'textarea' tags, we use 'value'.</p>
      <p>When we want to show something to a user, we use a 'section' and 'innerHTML'</p>
      <P>If we are dealing with an input' or 'textarea' use 'value'; for 'section' use innerHTML</p>
   
  </body>
</html>


#main.js
const links = [
  {
    label: "Week01 notes",
    url: "Week01/index.html"
  },
  {
    label: "Week02 notes",
    url: "Week02/index.html"
    } 
  
];

const ol = document.getElementById('assignments');      

links.forEach(link => {

  let li = document.createElement('li');

  let a = document.createElement('a');

  a.setAttribute('href', link.url);

  a.innerText = link.label;

  ol.appendChild(li);

});

